                    },
                    "description": {
                        "type": "string"
                    }
                },
                "required": [
                    "title",
                    "description"
                ]
            },
            "User": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string",
                        "example": "patrick",
                        "description": "Username. Alphanumeric, 2-24 characters."
                    },
                    "firstName": {
                        "type": "string",
                        "example": "John",
                        "description": "User's first name"
                    },
                    "lastName": {
                        "type": "string",
                        "example": "Smith",
                        "description": "User's last name"
                    },
                    "lists": {
                        "type": "object",
                        "description": "Lists created by user."
                    },
                    "refreshTokens": {
                        "type": "object"
                    }
                },
                "required": [
                    "username",
                    "firstName",
                    "lastName",
                    "lists",
                    "refreshTokens"
                ]
            },
            "List": {
                "type": "object",
                "properties": {}
            },
            "Todo": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string",
                        "example": "Clean out the fridge",
                        "description": "Title of the todo"
                    },
                    "description": {
                        "type": "string",
                        "example": "Be sure to remove all the shelving and scrub it.",
                        "description": "Optional. Description of the todo"
                    },
                    "createdBy": {
                        "description": "User. Creator of the todo.",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/User"
                            }
                        ]
                    },
                    "list": {
                        "description": "List the todo belongs to.",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/List"
                            }
                        ]
                    }
                },
                "required": [
                    "title",
                    "description",
                    "createdBy",
                    "list"
                ]
            },
