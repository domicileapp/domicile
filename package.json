{
  "name": "domicile",
  "version": "0.3.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}